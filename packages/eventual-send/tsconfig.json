{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "checkJs": true,
    "noEmit": true,
    "downlevelIteration": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "allowJs": true,
    "noImplicitAny": false,
    "useUnknownInCatchVariables": false,
    "skipLibCheck": false,
  },
  "include": [
    "*.js",
    "*.ts",
    "src/**/*.js",
    "src/**/*.ts"
  ]
}
